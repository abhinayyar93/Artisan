extends ../layouts/default

block content
	#myCarousel.carousel.slide(data-ride="carousel")
		//indicators
		ol.carousel-indicators
			li.active(data-target='#myCarousel', data-slide-to='0')
			li(data-target='#myCarousel', data-slide-to='1')
			li(data-target='#myCarousel', data-slide-to='2')
			li(data-target='#myCarousel', data-slide-to='3')

		//Wrapper for slides
		.carousel-inner(role="listbox")
			.item.active
				img(src=Homes.image.first, alt="")
				p.caption=Homes.content.first
					//- |	Geometrical patterns, retro designs, fine stripes or clear plains sets fashion trends for your home.
			.item
				img(src=Homes.image.second, alt="")
				p.caption=Homes.content.second
					//- |	Textile plains, a canvas structure, metallic plains or mosaic designs with metallic effects give your rooms a modern touch.
			.item
				img(src=Homes.image.third, alt="")
				p.caption=Homes.content.third
					//- |	A rich collection with structural and material effects alternating between style and bright.
			.item
				img(src=Homes.image.fourth, alt="")
				p.caption=Homes.content.fourth
					//- |	Motifs, material and volume effects illustrate a geometric collection that set contemporary moods for your rooms.
		//Left and right controls
		a.left.carousel-control(href="#myCarousel", role="button", data-slide="prev")
			span.glyphicon.glyphicon-chevron-left(aria-hidden="true")
			span.sr-only Previous
		a.right.carousel-control(href="#myCarousel", role="button", data-slide="next")
			span.glyphicon.glyphicon-chevron-right(aria-hidden="true")
			span.sr-only Next

	section
		.container
			.row.row1-height
				h1.home-heading=Homes.content.fifth 
				//- Decorate With Artisan
				p.home-para=Homes.content.sixth
					//- |	ARTISAN Furnishings Pvt Ltd, leader in importing and distributing & developing Wallcovering, provides with a varied range of commercial grade quality products for the Hospitality Industry, Corporate Buildings and Residential Market manufactured by the world’s leading manufacturers located in USA, Europe and Japan.

		.row.row-color
			.col-md-6.col-row-padd-marg
				img(src=Homes.image.fifth, width="100%")

			.col-md-5.col-md-offset-1.col-row-padd-marg.row-bootstrap
				.content
					h1.about-heading=Homes.content.seventh 
					.about-para=Homes.content.eighth
						//- | ARTISAN Furnishings Pvt Ltd imports and distributes premier Wall covering brands, and develops & markets a broad range of commercial grade quality products for the Hospitality Industry, Corporate Buildings and Residential Market manufactured by the world’s leading manufacturers located in USA, Europe and Japan. ARTISAN was established in the year 2003. Over the last 12 years, the Artisan network expanded to other cities in India with a wide network of over 500 distributors and dealers across the country.
					a.know-more-button(href="./au/ww are.htm") know more

		.row.row-color.col-row-padd-marg
			h3.home-heading.product=Homes.content.nine
			.col-md-4.col-row-padd-marg
				a(href=Homes.content.twelve, target="blank")
				img.product-image(src=Homes.image.thirteen)
			.col-md-4.col-row-padd-marg
				a(href=Homes.content.thirteen, target="blank")
				img.product-image(src=Homes.image.fourteen)
			.col-md-4.col-row-padd-marg
				a(href=Homes.content.fourteen, target="blank")
				img.product-image(src=Homes.image.fifteen)

		.row.row-color.row-about.col-row-padd-marg
			h3.home-heading.product=Homes.content.ten 
			.brands
			img(src=Homes.image.six, width="100%")
	
		.row-coverings.wall-padding
